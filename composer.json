{
  "name": "casparone/qiwi-laravel",
  "description": "Wrapper for QIWI payment SDK for Laravel 5",
  "keywords": ["qiwi", "payments", "processing", "qiwi sdk", "php", "merchant"],
  "homepage": "https://github.com/CasparOne/qiwi-laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Victor Volodin",
      "email": "vvolodin.victor@gmail.com",
      "homepage": "https://github.com/CasparOne"
    }
  ],
  "require": {
    "php": "^7.*|^8.*",
    "qiwi/bill-payments-php-sdk": "^0.1.0",
    "illuminate/support": "^8",
    "symfony/var-dumper": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "phpunit/phpunit-mock-objects": "~2.3.0"
  },
  "autoload": {
    "psr-4": {"Qiwi\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Qiwi\\Tests\\": "tests/"}
  }
}
